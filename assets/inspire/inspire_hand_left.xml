<mujoco model="h1">
    <compiler angle="radian" meshdir="./meshes/" />

    <option timestep="0.02" iterations="10" ls_iterations="50">
        <flag eulerdamp="disable" />
    </option>

    <default>
        <!-- disable all contacts and use explicit collision pairs -->
        <geom condim="1" contype="0" conaffinity="0" />
        <!-- with this setting, the response time is around 1s -->
        <position kp="300" dampratio="1.0" inheritrange="1" />
        <joint damping="0.0" armature="1.0" frictionloss="0.0" />
        <default class="mimic_left">
            <site size="0.01" type="sphere" rgba="1 0 0 1" group="0" />
        </default>
    </default>

    <asset>
        <mesh name="L_hand_base_link" content_type="model/stl" file="L_hand_base_link.STL" />
        <mesh name="Link11_L" content_type="model/stl" file="Link11_L.STL" />
        <mesh name="Link12_L" content_type="model/stl" file="Link12_L.STL" />
        <mesh name="Link13_L" content_type="model/stl" file="Link13_L.STL" />

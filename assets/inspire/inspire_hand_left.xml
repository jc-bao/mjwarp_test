                size="0.01 0.02 0.03" />
            <joint name="left_pos_x" type="slide" pos="0 0 0" axis="1 0 0" range="-1.0 1.0" />
            <joint name="left_pos_y" type="slide" pos="0 0 0" axis="0 1 0" range="-1.0 1.0" />
            <joint name="left_pos_z" type="slide" pos="0 0 0" axis="0 0 1" range="-1.0 1.0" />

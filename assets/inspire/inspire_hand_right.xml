    <position name="right_pos_x_position" joint="right_pos_x" />
    <position name="right_pos_y_position" joint="right_pos_y" />
    <position name="right_pos_z_position" joint="right_pos_z" />
    <position name="right_rot_x_position" joint="right_rot_x" />
    <position name="right_rot_y_position" joint="right_rot_y" />
    <position name="right_rot_z_position" joint="right_rot_z" />

    <!-- Thumb joints -->

    <position name="right_thumb_proximal_pitch_position" joint="right_thumb_proximal_pitch_joint" />
    <position name="right_thumb_intermediate_position" joint="right_thumb_intermediate_joint" />
    <position name="right_thumb_distal_position" joint="right_thumb_distal_joint" />

